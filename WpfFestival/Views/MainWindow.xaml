<Window x:Class="WpfFestival.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfFestival.Views"
        xmlns:prism ="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="650.4" Width="1216.4">
    <DockPanel >
        <ContentControl prism:RegionManager.RegionName="ContentRegion" HorizontalAlignment="Left" Margin="0,10,0,9.8" Width="884"/>

        <Button Content="Creer un festival" Command="{Binding GoToFestivalFormulaire}" CommandParameter="FestivalFormulaire"  Width="51" Margin="0,257,0,311.8"/>
        <Button Content="Button" Height="52" Command="{Binding GoToProgrammationFormulaire}" CommandParameter="ProgrammationFormulaire" VerticalAlignment="Top" Width="75" Margin="0,257,0,0"/>
        <Button Content="Modifier Scene" Command="{Binding GoToModifierScene}" CommandParameter="ModifierScene" Height="620.8" VerticalAlignment="Top" Width="75"/>
        <Button Content="Artiste" Command="{Binding GoToModifierArtiste}" CommandParameter="ModifierArtiste" Height="620.8" VerticalAlignment="Top" Width="75"/>



    </DockPanel>
</Window>
